{
    "files.associations": {
        "*.H": "cpp",
        "*.C": "cpp"
    },
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "python.testing.pytestArgs": [
        "."
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".vscode": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true
    },
    "hide-files.files": []
}